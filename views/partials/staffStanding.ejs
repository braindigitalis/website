<% if (user.staffTracking.details.standing === "Unmeasured") { %>
    <strong><%= __("page.staff.manager.standing") %></strong> <%= __("page.staff.manager.unmeasured") %>
<% } else if (user.staffTracking.details.standing === "Good") { %>
    <strong><%= __("page.staff.manager.standing") %></strong> <%= __("page.staff.manager.good") %>
<% } else if (user.staffTracking.details.standing === "Moderate") { %>
    <strong><%= __("page.staff.manager.standing") %></strong> <%= __("page.staff.manager.moderate") %>
<% } else if (user.staffTracking.details.standing === "Moderate-Bad") { %>
    <strong><%= __("page.staff.manager.standing") %></strong> <%= __("page.staff.manager.moderateBad") %>
<% } else if (user.staffTracking.details.standing === "Bad") { %>
    <strong><%= __("page.staff.manager.standing") %></strong> <%= __("page.staff.manager.bad") %>
<% } %>